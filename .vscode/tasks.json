{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Inspect Discord.JS Bot",
      "command": "node --inspect app.js",
      "options": { "cwd": "${workspaceFolder}" },
      "isBackground": true,
      "type": "shell",
      "problemMatcher": {
        "owner": "typescript",
        "fileLocation": "relative",
        "pattern": {
          "regexp": "asdfg",
          "file": 1,
          "location": 2,
          "severity": 3,
          "code": 4,
          "message": 5
        },
        "background": {
          "activeOnStart": false,
          "beginsPattern": "Bot online",
          "endsPattern": "Have to wait for"
        }
      }
    }
  ]
}
